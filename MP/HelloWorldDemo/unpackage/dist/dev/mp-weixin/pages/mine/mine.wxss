
.content {
	height: 100%;
	width: 100%;
}
.content {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.cell-item {
	width: 100px;
	height: 50px;
	background: orange;
	margin: 5px;
	
	display: -webkit-box;
	
	display: -webkit-flex;
	
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}

